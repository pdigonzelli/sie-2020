  define var tipo_mov as integer.
  define var tipo_mov_ex as integer.
  
  run get-seleccion in h_b_tt_eleccion_tipos_mov_existe (output tipo_mov, output tipo_mov_ex).
    
  find tipos_movimientos where tipos_movimientos.id_tipo_movimiento = tipo_mov no-lock no-error.
  if available tipos_movimientos then
    do:
        run value(tipos_movimientos.alta) (input tipo_mov, input tipo_mov_ex).
    end.
  
  /* run w_carga_etiquetas.w (input mov, input tipo_mov). */
  
  /*
  find tambores_produccion_jugo where rowid(tambores_produccion_jugo) = to-rowid(entry(i,elegidos)) no-error.
