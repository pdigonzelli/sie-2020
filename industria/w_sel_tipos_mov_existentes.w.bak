  apply ("window-close") to w-win.
END.

/* _UIB-CODE-BLOCK-END */
&ANALYZE-RESUME


&Scoped-define SELF-NAME BUTTON-6
&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL BUTTON-6 W-Win
ON CHOOSE OF BUTTON-6 IN FRAME F-Main /* Movimiento por lotes */
DO:
  define var mov as integer.
  define var tipo_mov as integer.
  
  run get-seleccion in h_b_tt_eleccion_tipos_mov_existe (output mov, output tipo_mov).
  run w_carga_etiquetas_lotes.w (input mov, input tipo_mov).
  

