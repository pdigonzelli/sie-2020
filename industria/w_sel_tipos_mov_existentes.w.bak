     do:
         assign tambores_produccion_jugo.id_empresa_destino = p_id_empresa
                tambores_produccion_jugo.id_sucursal_destino= p_id_sucursal
                tambores_produccion_jugo.id_lote_destino = p_id_lote.
      end.
               
APPLY "WINDOW-CLOSE":U TO CURRENT-WINDOW.
  */
END.

