&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE adm-create-objects W-Win  _ADM-CREATE-OBJECTS
PROCEDURE adm-create-objects :
/*------------------------------------------------------------------------------
  Purpose:     Create handles for all SmartObjects used in this procedure.
               After SmartObjects are initialized, then SmartLinks are added.
  Parameters:  <none>
------------------------------------------------------------------------------*/
  DEFINE VARIABLE adm-current-page  AS INTEGER NO-UNDO.

  RUN get-attribute IN THIS-PROCEDURE ('Current-Page':U).
  ASSIGN adm-current-page = INTEGER(RETURN-VALUE).

  CASE adm-current-page: 

    WHEN 0 THEN DO:
       RUN init-object IN THIS-PROCEDURE (
             INPUT  'n:/industria/bc_contratos_ff.w':U ,
             INPUT  FRAME F-Main:HANDLE ,
             INPUT  'Layout = ':U ,
             OUTPUT h_bc_contratos_ff ).
       RUN set-position IN h_bc_contratos_ff ( 2.43 , 4.00 ) NO-ERROR.
       RUN set-size IN h_bc_contratos_ff ( 6.43 , 139.00 ) NO-ERROR.

       RUN init-object IN THIS-PROCEDURE (
             INPUT  'n:/industria/bc_items_contratos_ff.w':U ,
             INPUT  FRAME F-Main:HANDLE ,
             INPUT  'Layout = ':U ,
             OUTPUT h_bc_items_contratos_ff ).
       RUN set-position IN h_bc_items_contratos_ff ( 10.29 , 4.00 ) NO-ERROR.
       RUN set-size IN h_bc_items_contratos_ff ( 6.71 , 128.00 ) NO-ERROR.

       RUN init-object IN THIS-PROCEDURE (
             INPUT  'n:/industria/bc_distribuciones_contratos.w':U ,
             INPUT  FRAME F-Main:HANDLE ,
             INPUT  'Layout = ':U ,
             OUTPUT h_bc_distribuciones_contratos ).
       RUN set-position IN h_bc_distribuciones_contratos ( 17.43 , 4.00 ) NO-ERROR.
       RUN set-size IN h_bc_distribuciones_contratos ( 6.71 , 54.00 ) NO-ERROR.

       /* Links to csmartbrowser h_bc_items_contratos_ff. */
       RUN add-link IN adm-broker-hdl ( h_bc_contratos_ff , 'Record':U , h_bc_items_contratos_ff ).

       /* Links to csmartbrowser h_bc_distribuciones_contratos. */
       RUN add-link IN adm-broker-hdl ( h_bc_items_contratos_ff , 'Record':U , h_bc_distribuciones_contratos ).

       /* Adjust the tab order of the smart objects. */
       RUN adjust-tab-order IN adm-broker-hdl ( h_bc_contratos_ff ,
             FILL-IN-1:HANDLE IN FRAME F-Main , 'AFTER':U ).
       RUN adjust-tab-order IN adm-broker-hdl ( h_bc_items_contratos_ff ,
             FILL-IN-2:HANDLE IN FRAME F-Main , 'AFTER':U ).
       RUN adjust-tab-order IN adm-broker-hdl ( h_bc_distribuciones_contratos ,
             h_bc_items_contratos_ff , 'AFTER':U ).
    END. /* Page 0 */

  END CASE.

END PROCEDURE.

