       RUN addLink ( h_dyntoolbar , 'Navigation':U , h_d002derechospermisosembarque ).

       /* Links to SmartDataBrowser h_b002derechospermisosembarque. */
       RUN addLink ( h_d002derechospermisosembarque , 'Data':U , h_b002derechospermisosembarque ).

       /* Links to SmartDataViewer h_v002derechospermisosembarque. */
       RUN addLink ( h_d002derechospermisosembarque , 'Data':U , h_v002derechospermisosembarque ).
       RUN addLink ( h_v002derechospermisosembarque , 'Update':U , h_d002derechospermisosembarque ).
       RUN addLink ( h_dyntoolbar , 'TableIO':U , h_v002derechospermisosembarque ).

       /* Adjust the tab order of the smart objects. */
