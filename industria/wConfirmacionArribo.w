       RUN adjustTabOrder ( h_dyntoolbar ,
             h_bqrtosnoconfirmados , 'AFTER':U ).
       RUN adjustTabOrder ( h_dynfilter ,
             h_dyntoolbar , 'AFTER':U ).
       RUN adjustTabOrder ( h_bitemsremito ,
             h_dynfilter , 'AFTER':U ).
       RUN adjustTabOrder ( h_dyntoolbar-2 ,
             h_bitemsremito , 'AFTER':U ).
       RUN adjustTabOrder ( h_vconfirmacionremito ,
