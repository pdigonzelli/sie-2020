             INPUT  'bqrtosnoconfirmados.w':U ,
             INPUT  FRAME fMain:HANDLE ,
             INPUT  'ScrollRemotenoNumDown0CalcWidthnoMaxWidth80FetchOnReposToEndyesDataSourceNamesUpdateTargetNamesLogicalObjectNameHideOnInitnoDisableOnInitnoObjectLayout':U ,
             OUTPUT h_bqrtosnoconfirmados ).
       RUN repositionObject IN h_bqrtosnoconfirmados ( 1.00 , 1.00 ) NO-ERROR.
       RUN resizeObject IN h_bqrtosnoconfirmados ( 8.33 , 66.00 ) NO-ERROR.

       RUN constructObject (
             INPUT  'ditemsremito.wDB-AWARE':U ,
             INPUT  FRAME fMain:HANDLE ,
