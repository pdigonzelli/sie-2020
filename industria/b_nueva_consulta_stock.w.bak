&ANALYZE-RESUME

&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE cargar_datos B-table-Win 
PROCEDURE cargar_datos :
/*------------------------------------------------------------------------------
  Purpose:     
  Parameters:  <none>
  Notes:       
------------------------------------------------------------------------------*/
define var v_tambores as integer.
define var h_con as handle.
define var p_suc as integer initial 96.

for each tt_stock.
    delete tt_stock.
end.


run get-container (output h_con).
run dame_sucursal in h_con (output p_suc).


for each tambores_industria where id_sucursal_ubicacion = p_suc
                              and id_locacion_ubicacion = 4
                              and (tambores_industria.id_tipotambor = 3
                                    or tambores_industria.id_tipotambor = 6) 
                               break by tambores_industria.id_lote.

        accumulate tambores_industria.id_lote (count by tambores_industria.id_lote) .
        v_tambores = v_tambores + 1.
        accumulate tambores_industria.kilos_tambor (total by tambores_industria.id_lote).
        
    if last-of(tambores_industria.id_lote) then                               
        do:
            
            find envases_prod of tambores_industria no-lock no-error.
            find productos_terminados of tambores_industria no-lock no-error.
            find calidades of tambores_industria no-lock no-error.
            find contratos where contratos.id_contrato      = tambores_industria.id_contrato_of
                             and contratos.id_tipo_contrato = tambores_industria.id_tipocontrato_of
                             and contratos.anio             = tambores_industria.anio_of no-lock no-error.
            find items_contratos where items_contratos.id_contrato      = tambores_industria.id_contrato_of
                                   and items_contratos.id_tipo_contrato = tambores_industria.id_tipocontrato_of
                                   and items_contratos.anio             = tambores_industria.anio_of 
                                   and items_contratos.item             = tambores_industria.item_of 
                                   no-lock no-error.
            find clientes of contratos no-lock no-error.
            find r_productos_calidad where r_productos_calidad.id_articulo = tambores_industria.id_articulo
                                       and r_productos_calidad.id_calidad = tambores_industria.id_calidad
                                       no-lock no-error. 
                                         
            create tt_stock.
            assign tt_stock.id_lote     = tambores_industria.id_lote 
                   tt_stock.id_tipotambor = tambores_industria.id_tipotambor
                   tt_stock.anio        = STRING(tambores_industria.anio)
                   tt_stock.envase      = envases_prod.abreviatura
                   tt_stock.articulo    = productos_terminados.abreviatura
                   tt_stock.id_articulo = productos_terminados.id_articulo.
                   if available calidades then
                    do:
                        assign tt_stock.calidad     = calidades.abreviatura
                               tt_stock.id_calidad  = calidades.id_calidad.
                    end.                  
                   
                   if available contratos then
                    do:
                         assign tt_stock.orden_f     = contratos.orden_fabricacion 
                                tt_stock.contrato    = contratos.id_contrato
                                tt_stock.cliente     = clientes.nombre
                                tt_stock.sem_des     = items_contrato.semana_entrega
                                tt_stock.sem_has     = items_contrato.semana_entrega_hasta
                                tt_stock.anio_sem    = items_contrato.anio_semana.
                    end.
            assign tt_stock.tambores    = (accum count by tambores_industria.id_lote tambores_industria.id_lote)
                   tt_stock.kilos       = (accum total by tambores_industria.id_lote tambores_industria.kilos_tambor).
                   if available r_productos_calidad then
                   assign tt_stock.kilos_400   = (accum total by tambores_industria.id_lote tambores_industria.kilos_tambor) * 
                                          r_productos_calidad.coeficiente.
            
            v_tambores = 0.
        end.
        
        
    
        
                                    
end.                                    

run dispatch in this-procedure ('open-query').

END PROCEDURE.

