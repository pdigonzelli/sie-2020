&Scoped-define SELF-NAME gDialog
&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL gDialog gDialog
ON WINDOW-CLOSE OF FRAME gDialog /* Seleccion Archivo Permisos */
DO:  
  /* Add Trigger to equate WINDOW-CLOSE to END-ERROR. */
  APPLY "END-ERROR":U TO SELF.
END.

/* _UIB-CODE-BLOCK-END */
&ANALYZE-RESUME


&Scoped-define SELF-NAME Btn_OK
&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL Btn_OK gDialog
ON CHOOSE OF Btn_OK IN FRAME gDialog /* OK */
DO:
  DEFINE VARIABLE hLib AS HANDLE     NO-UNDO.
  DEFINE VARIABLE cPro AS CHARACTER  NO-UNDO.

  DEFINE VARIABLE hLibCom AS HANDLE.
  RUN libCommonFunctions.p PERSISTENT SET hLibCom.
  hLib   = DYNAMIC-FUNCTION('getPersistentLib' IN hLibCom, 'libReportes.p'). 
  DELETE OBJECT hLibCom.

  IF optOpcion:SCREEN-VALUE = "1" THEN
    cPro = "printPackingListsFromExcel".

  IF optOpcion:SCREEN-VALUE = "2" THEN
    cPro = "printPackingListSinFactura".


  RUN VALUE(cPro) IN hLib (fiArchivo:SCREEN-VALUE, INTEGER(fiRows:SCREEN-VALUE)).


END.

